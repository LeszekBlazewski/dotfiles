Host *
  ServerAliveInterval 50
  useKeychain yes
  AddKeysToAgent yes
  IdentityFile ~/.ssh/key-a
  IdentityFile ~/.ssh/key-b
  # Add all the keys which aren't git related

Host jumpbox.com
  ForwardAgent yes

# Do this for each git user/host you are using
Host github.com-my
  AddKeysToAgent yes
  UseKeychain yes
  IdentityFile ~/.ssh/key-gh
  HostName github.com
  User git
