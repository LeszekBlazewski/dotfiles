Host jumpbox.com
  IdentityFile ~/.ssh/key-X
  ForwardAgent yes

Host 1.2.3.4
  IdentityFile ~/.ssh/key-X

# Do this for each git user/host you are using
Host github.com-my
  IdentityFile ~/.ssh/key-gh
  HostName github.com
  User git

# Generic options at the end
Host *
  ServerAliveInterval 50
  useKeychain yes
  AddKeysToAgent yes
